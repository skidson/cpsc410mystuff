<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ca.ubc.cpsc.mystuff.model.FriendRequest" table="FriendRequest">
		<id name="requestID" column="requestID">
			<generator class="assigned"/>
		</id>
		<property name="receiverID" column="receiverID"/>
		<property name="senderID" column="senderID"/>
   </class>
</hibernate-mapping>