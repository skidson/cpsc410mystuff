<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" 
	version="2.5">
	
	<servlet>
		<servlet-name>MyStuffController</servlet-name>
		<servlet-class>ca.ubc.cpsc.mystuff.controller.MyStuffController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyStuffController</servlet-name>
		<url-pattern>/mystuff</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>LoginController</servlet-name>
		<servlet-class>ca.ubc.cpsc.mystuff.controller.LoginController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginController</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	
  <welcome-file-list>
    <welcome-file>view.jsp</welcome-file>
  </welcome-file-list>
</web-app>